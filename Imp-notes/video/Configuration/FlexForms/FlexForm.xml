<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<main>
			<ROOT>
				<TCEforms>
					<sheetTitle>Main Settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
                    <settings.storagePID>
                        <TCEforms>
                            <label>LLL:EXT:video/Resources/Private/Language/locallang_flexform.xlf:flexforms_general.storagePID</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <autoSizeMax>10</autoSizeMax>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <default></default>
                            </config>
                        </TCEforms>
                    </settings.storagePID>
                    <settings.limit>
						<TCEforms>
							<label>LLL:EXT:video/Resources/Private/Language/locallang_flexform.xml:flexforms_general.limit</label>
							<config>
								<type>input</type>
								<size>2</size>
								<max>2</max>
                                <eval>num</eval>
							</config>
						</TCEforms>
					</settings.limit>  
				
			    <settings.img>
					    <TCEforms type="array">
					        <label>Image</label>
					        <config type="array">
					            <type>group</type>
					            <internal_type>file</internal_type>
					            <allowed>jpg,jpeg,png</allowed>
					            <max_size>10240</max_size>
					            <uploadfolder>uploads/tx_videos</uploadfolder>
					            <show_thumbs>1</show_thumbs>
					            <size>1</size>
					            <maxitems>1</maxitems>
					            <minitems>0</minitems>
					        </config>
					    </TCEforms>
					</settings.img>
					                
                   
                </el>
			</ROOT>
		</main>

	</sheets>
</T3DataStructure>